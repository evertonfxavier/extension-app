{
  "name": "PackTracking",
  "description": "É possível rastrear suas encomendas enviadas pelos Correios",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./static/js/content.js"],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["<all_urls>"]
}
